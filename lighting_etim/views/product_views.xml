<odoo>
    <record id="product_view_form_inherit_etim" model="ir.ui.view">
        <field name="name">product_view.form.inherit.etim</field>
        <field name="model">lighting.product</field>
        <field name="inherit_id" ref="lighting.product_form_view"/>
        <field name="arch" type="xml">
            <xpath expr="//notebook" position="inside">
                <page string="ETIM">
                    <group>
                        <field name="class_id"/>
                    </group>
                    <group string="Features">
                        <field name="feature_ids" nolabel="1"/>
                    </group>
                </page>
            </xpath>
        </field>
    </record>
</odoo>